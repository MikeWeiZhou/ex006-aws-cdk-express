{
  "root": true,
  "env": {
    "es2021": false,
    "node": true
  },
  "extends": [
    "airbnb-base",
    "plugin:import/typescript"
  ],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": 9,
    "sourceType": "module"
  },
  "plugins": [
    "@typescript-eslint"
  ],
  "rules": {
    "import/extensions": "off",
    "import/prefer-default-export": "off",
    "class-methods-use-this": "off"
  }
}
