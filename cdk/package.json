{
  "name": "express-api-ref-cdk",
  "version": "0.1.0",
  "scripts": {
    "build": "npm run clean:build && tsc",
    "bootstrap": "cdk bootstrap",
    "synth": "cdk synth",
    "deploy:dev": "npm run clean:cache && cdk deploy dev-api-usw2",
    "destroy:dev": "cdk destroy dev-api-usw2",
    "clean:all": "npm run clean:build && npm run clean:cache",
    "clean:build": "rimraf \"!(node_modules)/**/*+(.js|.d.ts)\" \"*.js\" \"*.d.ts\"",
    "clean:cache": "rimraf \"./cdk.out\" \"./cdk.context.json\""
  },
  "devDependencies": {
    "@aws-cdk/assert": "^1.102.0",
    "@types/node": "10.17.27",
    "aws-cdk": "^1.102.0",
    "rimraf": "^3.0.2",
    "ts-node": "^9.0.0",
    "typescript": "~3.9.7"
  },
  "dependencies": {
    "@aws-cdk/aws-ec2": "^1.102.0",
    "@aws-cdk/aws-ecs": "^1.102.0",
    "@aws-cdk/aws-ecs-patterns": "^1.102.0",
    "@aws-cdk/aws-rds": "^1.102.0",
    "@aws-cdk/aws-ssm": "^1.102.0",
    "@aws-cdk/core": "^1.102.0",
    "source-map-support": "^0.5.16"
  }
}
